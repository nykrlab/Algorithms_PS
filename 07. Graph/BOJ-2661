n = int(input())
result = []

def check(result, cnt):
    for k in range(cnt):
        sliceTemp = result[k:]
        for i in range(1, len(sliceTemp)//2+1):
            checkV = sliceTemp[:i]
            if checkV == sliceTemp[i:i+i]:
                return False
    return True

def backTracking(cnt):
    if not check(result, cnt):
        return -1
    if cnt==n:
        print(*result, sep="")
        return 0
    for i in range(1,4):
        result.append(i)
        if backTracking(cnt+1)==0:
            return 0
        result.pop()
        
backTracking(0)
