n,k = map(int,input().split()) #10 4200
m=[]
num=0

for i in range(n): # 1 5 10 50 100 500 1000 ~ 50000 동전 입력 받기
  m.append(int(input()))

for i in range(n-1,-1,-1): #n-1부터 0까지 // 가격 큰 것부터 
  if k==0: #k가 가격 
    break
  if m[i]>k: # 주어진 동전이 가격보다 높을 때
    continue 
  num += k//m[i]
  k %= m[i] # 가격 갱신

print(num)
